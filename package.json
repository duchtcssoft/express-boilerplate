{
  "name": "express-boilerplate",
  "version": "1.7.0",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "start:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "build:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "build:prod": "cross-env NODE_ENV=production tsc -b && babel build -d build",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "watch:pm2": "pm2 start ecosystem.config.json --no-daemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-mongo-sanitize": "2.1.0",
    "express-rate-limit": "5.3.0",
    "fs-extra": "10.0.0",
    "helmet": "4.1.0",
    "http-status": "1.4.0",
    "joi": "17.3.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "mongoose": "5.13.5",
    "morgan": "1.9.1",
    "nodemailer": "6.3.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pm2": "5.1.0",
    "swagger-jsdoc": "6.0.8",
    "swagger-ui-express": "4.1.6",
    "validator": "13.0.0",
    "winston": "3.2.1",
    "xss-clean": "0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "5.1.3",
    "@types/faker": "5.5.7",
    "@types/jest": "26.0.15",
    "@types/morgan": "1.9.3",
    "@types/node": "12.0.0",
    "@types/nodemailer": "6.4.4",
    "@types/passport": "1.0.7",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.6.3",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "babel-jest": "27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "coveralls": "3.0.7",
    "cross-env": "7.0.0",
    "csstype": "^3.0.8",
    "eslint": "7.0.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "faker": "5.1.0",
    "husky": "7.0.0",
    "jest": "26.0.1",
    "lint-staged": "11.0.0",
    "node-mocks-http": "1.8.0",
    "nodemon": "2.0.0",
    "prettier": "2.0.5",
    "supertest": "6.0.1",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=6.x",
    "yarn": ">=1.21.3"
  }
}
